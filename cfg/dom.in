set g_dom_default_teams		2 "default number of teams for maps that aren't domination-specific"
seta g_dom_teams_override		0 "use a specific number of teams in domination games (minimum 2), disables dom_team entities"
set g_dom_disable_frags		0 "players can't get frags normally, only get points from kills"
set g_dom_point_amt		0 "override: how many points to get per ping"
set g_dom_point_fullbright	0 "domination point fullbright"
set g_dom_point_rate		0 "override: how often to give those points"
set g_dom_point_capturetime	0.1 "how long it takes to capture a point (given no interference)"
set g_dom_point_glow		0 "domination point glow (warning, slow)"
set g_dom_point_capturetime 1
set g_dom_scorelimit 200 "Scorelimit for domination, when empty, g_scorelimit using, <= 0 mean no limit"
set g_dom_timelimit "" "Timelimit for domination, when empty, g_timelimit using, <= 0 mean no limit"
set g_dom_leadlimit "0" "Leadlimit for domination, when empty, g_leadlimit using, <= 0 mean no limit"
alias g_domination_point_limit "cvardeprecated g_domination_point_limit g_dom_scorelimit; g_dom_scorelimit $1"
alias g_domination_default_teams "cvardeprecated g_domination_default_teams g_dom_default_teams; g_dom_default_teams $1"
alias g_domination_teams_override "cvardeprecated g_domination_teams_override g_dom_teams_override; g_dom_teams_override $1"
alias g_domination_disable_frags "cvardeprecated g_domination_disable_frags g_dom_disable_frags; g_dom_disable_frags $1"
alias g_domination_point_amt "cvardeprecated g_domination_point_amt g_dom_point_amt; g_dom_point_amt $1"
alias g_domination_point_fullbright "cvardeprecated g_domination_point_fullbright g_dom_point_fullbright; g_dom_point_fullbright $1"
alias g_domination_point_rate "cvardeprecated g_domination_point_rate g_dom_point_rate; g_dom_point_rate $1"
alias g_domination_point_capturetime "cvardeprecated g_domination_point_capturetime g_dom_point_capturetime; g_dom_point_capturetime $1"
alias g_domination_point_glow "cvardeprecated g_domination_point_glow g_dom_point_glow; g_dom_point_glow $1"
