///// BEGIN Handgrenades mutator /////

// General settings
set g_nadgets 0 "Enables Nadgets (bound to the hook key). Conflicts with g_grappling_hook and g_jetpack"
set g_nadgets_ammo 1 "Amount of nadgets the player starts with"
set g_nadgets_initial_regen 0 "Apply x seconds worth of regeneration on top of g_nadgets_ammo. If g_nadgets_regen is 0, the remainder is rounded"
set g_nadgets_regen 1 "If 1, enable nadget regeneration. The regeneration time is calculated by the following formula: regen_time * (1 + pow(current_ammo, regen_exponent) * regen_factor). These variables are set per nadget type."
set g_nadgets_regen_damagepause 0 "Suspend regeneration for this much seconds after taking damage"
set g_nadgets_regen_frozenfactor 1 "Multiplies the regeneration rate by this factor while frozen in freezetag"
set g_nadgets_regen_compensate 1 "After using a nadget, reduce the regeneration time based on the time spent regenerating the next one. If 0, unfinished regens will be lost completely"
set g_nadgets_pickup 1 "Enables +1 nadget pickup items"
set g_nadgets_pickup_random_bonus 0 "If enabled, nadget items give a random bonus nadget instead of +1 nadget ammo. Value controls how much bonuses to give"
set g_nadgets_pickup_regenvalue 30 "If >0, the nadget pickup items will grant this much seconds worth of regen instead of 1 nadget"
set g_nadgets_type "normal" "The default grenade type. Currently, normal and napalm are available"
set g_nadgets_type_selectable "normal remote plasma laser raze transloc shield linker sentry random" "A list of nadget types that players may pick. If empty, the default is always used"
set g_nadgets_type_selectable_primary "normal remote plasma laser raze random" "A list of nadget types that players may pick as their primary in g_nadgets_double mode."
set g_nadgets_type_selectable_secondary "transloc shield linker sentry random" "A list of nadget types that players may pick as their primary in g_nadgets_double mode."
set g_nadgets_double 0 "Players have 2 different nadgets at once"
set g_nadgets_detonate_on_death 0 "If enabled, all player's detonable grenades will be automatically triggered on death"
set g_nadgets_remove_on_death 0 "If enabled, all player's released nadgets are removed on death. Set to 2 to also remove them on getting frozen or imprisoned"
set g_nadgets_drop_on_death 0 "If enabled, players will drop their nadget on the ground if they die while charging it up"
set g_nadgets_detonatelaunches 0 "Use the detonation (dropweapon) button to launch the grenades"
set g_nadgets_storage_limit_factor 2 "How many nadgets can players store overall, based on regen_limit. e.g. a value of 2 means twice as much as regen_limit"

